  <script  src="https://code.jquery.com/jquery-3.3.1.js"  integrity="sha256-2Kok7MbOyxpgUVvAk/HJ2jigOSYS2auK4Pfzbm7uH60="  crossorigin="anonymous"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" ></script>
  <script src ="/js/contact.js"></script>
  <script>
  $(document).ready(function () {
      $.getJSON("https://api.mlab.com/api/1/databases/pharmacy-contact/collections/contact?apiKey=dI9gXrgAznHkTgvdNOqCp_WKAwZD2KON",function (data) {
      //  console.log(data);
        var message_data ='';
        $.each(data, function (key,value) {
            message_data += '<tr id="data">';
            message_data += '<td>' +value.name+'</td>';
            message_data += '<td>' +value.tel+'</td>';
            message_data += '<td>' +value.email+'</td>';
            message_data += '<td>' +value.message+'</td>';
            message_data += '<tr>';
        });
        $('#messages_table').append(message_data);
      });

  });
  </script>
  <script src ="/js/pagination.js"></script>
